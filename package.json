{
  "name": "webpack-userscript-template",
  "description": "Build your UserScript with webpack",
  "version": "0.0.1",
  "author": {
    "name": "Trim21",
    "email": "trim21me@gmail.com"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "scripts": {
    "lint": "eslint src/**/*.js src/**/*.ts",
    "preversion": "npm run lint",
    "postversion": "git push --follow-tags",
    "anylize": "cross-env npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.production.js",
    "dev": "webpack --mode development --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trim21/webpack-userscript-template"
  },
  "private": true,
  "dependencies": {
    "axios": "0.19.2",
    "axios-userscript-adapter": "0.0.4",
    "jquery": "^3.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "autoprefixer": "9.7.5",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "html-loader": "1.1.0",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "postcss-loader": "3.0.0",
    "style-loader": "1.1.3",
    "ts-loader": "7.0.1",
    "typescript": "3.8.3",
    "userscript-metadata-webpack-plugin": "0.0.6",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "3.6.1",
    "webpack-cli": "3.3.11",
    "webpack-livereload-plugin": "2.3.0",
    "webpack-merge": "4.2.2"
  }
}
