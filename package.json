{
  "name": "webpack-userscript-template",
  "description": "Build your UserScript with webpack",
  "version": "0.0.1",
  "author": {
    "name": "Trim21",
    "email": "trim21me@gmail.com"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "comma-dangle": "off"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "postversion": "npm run build && git add .",
    "anylize": "cross-env npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.production.js",
    "dev": "webpack --mode development --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trim21/webpack-userscript-template"
  },
  "private": true,
  "dependencies": {
    "axios": "0.19.2",
    "axios-userscript-adapter": "0.0.4"
  },
  "devDependencies": {
    "autoprefixer": "9.7.4",
    "cross-env": "7.0.1",
    "css-loader": "3.4.2",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "html-loader": "0.5.5",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "postcss-loader": "3.0.0",
    "style-loader": "1.1.3",
    "userscript-metadata-webpack-plugin": "0.0.3",
    "webpack": "4.42.0",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.11",
    "webpack-livereload-plugin": "2.3.0",
    "webpack-merge": "4.2.2"
  }
}
