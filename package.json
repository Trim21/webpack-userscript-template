{
  "name": "webpack-userscript-template",
  "description": "Build your UserScript with webpack",
  "version": "0.0.1",
  "author": {
    "name": "Trim21",
    "email": "trim21me@gmail.com"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ],
  "eslintIgnore": [
    "dist/*.js"
  ],
  "eslintConfig": {
    "extends": "standard"
  },
  "scripts": {
    "postversion": "npm run build && git add .",
    "anylize": "cross-env npm_config_report=true npm run build",
    "build": "webpack --mode production --config config/webpack.config.production.js",
    "dev": "webpack --mode development --watch --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Trim21/webpack-userscript-template"
  },
  "private": false,
  "devDependencies": {
    "autoprefixer": "9.7.1",
    "cross-env": "6.0.3",
    "css-loader": "3.2.0",
    "eslint": "5.16.0",
    "eslint-config-standard": "11.0.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "html-loader": "0.5.5",
    "postcss-loader": "2.1.6",
    "style-loader": "0.21.0",
    "userscript-metadata-webpack-plugin": "0.0.3",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "2.1.5",
    "webpack-merge": "4.2.2",
    "webpack-watch-livereload-plugin": "0.0.1"
  },
  "dependencies": {
    "axios": "0.19.0",
    "axios-userscript-adapter": "0.0.3"
  }
}
